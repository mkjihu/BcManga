<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
        
    <!-- 选中状态  -->
    <item android:state_checked="true" android:color="@color/keep_btcolor2"/>
    
    <!-- 按下动作 -->
	<item android:state_pressed="true" android:color="@color/keep_btcolor2"/>
	
	<!-- 聚焦 -->
    <item android:state_focused="true" android:color="@color/keep_btcolor2"/>
    
    <!-- 是否可用  否-->
    <item android:state_enabled="false" android:color="@color/keep_btcolor2"/>
    
    <!-- 是否可用  可-->
    <item android:state_enabled="true" android:color="@color/keep_btcolor3"/>
    
    <!-- 一般 -->
    <item  android:color="@color/keep_btcolor3" />
    
</selector>
